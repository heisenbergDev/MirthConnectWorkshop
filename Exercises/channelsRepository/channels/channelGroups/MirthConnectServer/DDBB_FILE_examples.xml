              <com.mirth.connect.plugins.javascriptrule.JavaScriptRule>
                <name>Only messages with enough data will be stored on database.</name>
                <sequenceNumber>0</sequenceNumber>
                <script>// Looking for Hospital Medical Record Number HMRN in the patinetId list //
for each (patientId in msg[&apos;PID&apos;][&apos;PID.3&apos;]) {
	if (patientId[&apos;PID.3.4&apos;].toString() == &apos;HMRN&apos;) {
		var patientId = &apos;&apos; + patientId[&apos;PID.3.1&apos;].toString();
	}
}

// Patient name //
var patientName = &apos;&apos; + msg[&apos;PID&apos;][&apos;PID.5&apos;][&apos;PID.5.2&apos;].toString();

// Patient familyname //
